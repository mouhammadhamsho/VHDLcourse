`timescale 1 ps/ 1 ps
module RAM_vlg_sample_tst(
	clock,
	data,
	rdaddress,
	wraddress,
	wren,
	sampler_tx
);
input  clock;
input [7:0] data;
input [4:0] rdaddress;
input [4:0] wraddress;
input  wren;
output sampler_tx;

reg sample;
time current_time;
always @(clock or data or rdaddress or wraddress or wren)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

module RAM_vlg_check_tst (
